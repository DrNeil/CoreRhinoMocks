    </copyright>
    <repository type="git" url="https://github.com/rubicon-oss/CoreRhinoMocks.git" />
    <tags>tdd mocking mocks unittests unittesting</tags>
    <license type="expression">BSD-3-Clause</license>
  </metadata>
  <files>
    <file src="bin\Release\net35\Rhino.Mocks.dll" target="lib/net35" />
    <file src="bin\Release\netcoreapp3.1\Rhino.Mocks.dll" target="lib/netcoreapp3.1" />
  </files>
</package>
